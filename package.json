{
  "name": "minha-carteira",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\" --config ./.prettierrc.json",
    "preview": "vite preview",
    "prepare": "husky install",
    "type-check": "tsc --project tsconfig.ci.json --pretty",
    "type-check:watch": "npm run type-check -- --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@react-firebase/auth": "^0.2.10",
    "axios": "^1.7.2",
    "firebase": "^10.12.3",
    "history": "^5.3.0",
    "motion": "^12.23.19",
    "polished": "^4.3.1",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.52.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.24.1",
    "react-text-mask": "^5.5.0",
    "recharts": "^2.12.7",
    "styled-components": "^6.1.11",
    "styled-system": "^5.1.5",
    "styled-theming": "^2.2.0",
    "text-mask-addons": "^3.8.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-text-mask": "^5.4.14",
    "@types/styled-system": "^5.1.22",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^8.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
